{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "ESNext",
    "useDefineForClassFields": true,
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "allowJs": false,
    "skipLibCheck": false,
    "esModuleInterop": false,
    "noImplicitAny": false,
    "allowSyntheticDefaultImports": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "paths": {
      "app/*": ["src/app/*"],
      "components/*": ["src/components/*"],
      "hooks/*": ["src/hooks/*"],
      "styles/*": ["src/styles/*"],
      "pages/*": ["src/pages/*"],
      "routes/*": ["src/routes/*"],
      "services/*": ["src/services/*"],
      "libs/*": ["src/libs/*"],
      "types/*": ["src/types/*"],
      "config/*": ["src/config/*"]
    }
  },
  "include": ["./src"]
}
